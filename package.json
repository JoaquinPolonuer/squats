{
  "name": "Rocker",
  "version": "0.1.0",
  "description": "tassssrial",
  "author": "Polo",
  "main": "public/electron.js",
  "build": {
    "appId": "display.menu"
  },
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.4.1",
    "builder": "^5.0.0",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.1.0",
    "electron-reload": "^1.5.0",
    "python-shell": "^1.0.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "reactstrap": "^8.4.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"py python/server.py\" \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"  \"py python/detect.py\"",
    "devMode": "concurrently \"py python/server.py\" \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^13.6.6",
    "electron-builder": "^22.4.1",
    "wait-on": "^4.0.1"
  }
}
